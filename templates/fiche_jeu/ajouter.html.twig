{% extends 'base.html.twig' %}

{% block title %}Ajouter Fiche{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <div id="tableJeuForm" class="col-lg-12">
                <h3 id="titleFormJeu" class="borderPurple cardTitreJeu">Ajouter une fiche</h3>
                {{ form_start(form,{'attr': {'autocomplete': 'off'}}) }}
                <div class="card">
                    <h3 class="card-header cardHeaderRoll">Votre Fiche</h3>
                    <div class="card-body">
                        {{ form_widget(form) }}
                        {{ form_end(form) }}
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
